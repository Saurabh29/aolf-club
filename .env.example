# Google Maps API Key (required for Places Autocomplete)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=aolf-club-locations
AWS_REGION=us-east-1

# For local development with DynamoDB Local:
# Set this to use local DynamoDB instance (docker run -p 8000:8000 amazon/dynamodb-local)
DYNAMODB_LOCAL_ENDPOINT=http://localhost:8000

# AWS credentials for DynamoDB Local (can be any non-empty string)
# For production, use proper IAM credentials or instance roles
AWS_ACCESS_KEY_ID=local
AWS_SECRET_ACCESS_KEY=local
